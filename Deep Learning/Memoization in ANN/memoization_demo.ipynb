        "# fibonacci Series\n",
        "\n",
        "import time\n",
        "def fib(n):\n",
        "    if n == 1 or n == 0:\n",
        "        return 1\n",
        "    else:\n",
        "      return fib(n-1) + fib(n-2)\n"
